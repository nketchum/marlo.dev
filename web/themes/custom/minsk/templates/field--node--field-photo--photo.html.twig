{#
/**
 * @file
 * Overidden theme override for a field.
 */
#}
{%
  set classes = [
    'field',
    'field--name-' ~ field_name|clean_class,
    'field--type-' ~ field_type|clean_class,
    'field--label-' ~ label_display,
  ]
%}

{% for item in items %}
  <figure{{ attributes.addClass(classes, 'field--item') }}{{ attributes.setAttribute('itemprop', 'associatedMedia') }}{{ attributes.setAttribute('itemscope', '') }}{{ attributes.setAttribute('itemtype', 'http://schema.org/ImageObject') }}>{{ item.content }}</figure>
{% endfor %}
